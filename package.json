{
  "name": "pdf-to-image-generator",
  "version": "0.0.6",
  "description": "Performant Node.js library to convert PDF file/buffer pages to PNG or JPEG files/buffers.",
  "keywords": [
    "pdf",
    "pdf to png",
    "pdf to jpeg",
    "pdf to image",
    "pdf file to image",
    "pdf buffer to image"
  ],
  "homepage": "https://github.com/GreenFlag31/pdf-to-image-generator#readme",
  "bugs": {
    "url": "https://github.com/GreenFlag31/pdf-to-image-generator/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GreenFlag31/pdf-to-image-generator.git"
  },
  "license": "MIT",
  "author": "Manu Claeys <unam31@gmail.com>",
  "main": "index.js",
  "types": "index.d.ts",
  "files": [
    "**"
  ],
  "scripts": {
    "build": "tsc && @powershell copy './package.json' 'dist/' && @powershell copy 'README.md' 'dist/' ",
    "pretest": "npm run clean && npm run build:test",
    "test": "mocha",
    "start:dev": "tsc-watch --onSuccess \"node dist/index.js\""
  },
  "dependencies": {
    "canvas": "^2.11.2",
    "pdfjs-dist": "^4.5.136"
  },
  "devDependencies": {
    "@types/chai": "^4.3.12",
    "@types/express": "^4.17.21",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.11.27",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "chai": "^4.4.1",
    "chai-as-promised": "^7.1.1",
    "mocha": "^10.3.0",
    "ts-node": "^10.9.2",
    "tsc-watch": "^6.2.0",
    "typedoc": "^0.26.5",
    "typedoc-github-theme": "^0.1.1",
    "typescript": "^5.4.2"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
