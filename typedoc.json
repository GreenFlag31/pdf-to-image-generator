{
  "entryPoints": ["src/index.ts"],
  "out": "docs",
  "projectDocuments": ["./performance.md"],
  "exclude": ["**/__tests__/**", "**/*.spec.ts", "**/node_modules/**"],
  "includeVersion": true, // Inclure la version du package dans la documentation
  "name": "pdf-to-image-generator", // Nom de votre librairie
  "lang": "en", // Langue de la documentation
  "excludeExternals": true, // Exclure les fichiers externes de la documentation
  "excludeInternal": true, // Exclure les membres marqués comme internes

  "plugin": ["typedoc-github-theme"],
  "theme": "typedoc-github-theme", // Thème par défaut pour la documentation

  "disableSources": true, // Désactiver l'affichage des sources
  "hideGenerator": true // Masquer le générateur de TypeDoc dans la documentation
}
